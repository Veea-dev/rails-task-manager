<h1>These are all tasks</h1>
<ul>
  <% @tasks.each do |task| %>
    <li>
      <%= link_to task.title, task_path(task)%>
      <%= link_to 'Delete', task_path(task), method: :delete, data: { confirm: 'Are you sure you want to delete this restaurant?' } %>
    </li>
  <% end %>
</ul>
<div class = "button"><%= link_to 'Create new task', new_task_path(@task)%>
</div>
